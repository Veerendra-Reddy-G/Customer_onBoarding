{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@/components/*": ["./src/components/*"],
      "@/context/*": ["src/app/context/*"]
    }
  },
  "include": ["src/**/*", "next-env.d.ts", "**/*.js", "**/*.jsx"],
  "exclude": ["node_modules", ".next"]
}